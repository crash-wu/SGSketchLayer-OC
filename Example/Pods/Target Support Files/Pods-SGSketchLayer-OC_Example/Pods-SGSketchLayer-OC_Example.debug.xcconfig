CLANG_ENABLE_MODULES = YES
ENABLE_BITCODE = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) $(HOME)/Library/SDKs/ArcGIS/iOS
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/SGSketchLayer_OC.framework/Headers"
OTHER_LDFLAGS = $(inherited) -l"ObjC" -l"c++" -framework "ArcGIS" -framework "SGSketchLayer_OC"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-SGSketchLayer-OC_Example
PODS_ROOT = ${SRCROOT}/Pods